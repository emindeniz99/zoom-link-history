{
	"name": "Zoom History",
	"description": "Find your latest zoom links",
	"version": "0.1",
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "latestzoom.png"
	},
	"permissions": ["storage", "history"],

	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}
