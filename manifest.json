{
	"name": "Zoom History",
	"description": "Find your latest zoom links",
	"version": "0.1.1",
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "latestzoom.png"
	},
	"permissions": ["storage"],
	"manifest_version": 2,
	"content_scripts": [
		{
		  "matches": ["*://zoom.us/j/*","*://*.zoom.us/j/*"],
		  "js": ["content.js"]
		}
	  ]
	 
}
